    <select id="selectByZongDuan" resultMap="vehicleVOResultMap" parameterType="java.lang.String" >
        select zongduanid from anbiao_vehicle
        where is_deleted = 0
            and zongduanid = #{id}
        group by
            zongduanid
        LIMIT 1
    </select>

